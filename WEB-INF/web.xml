<?xml version="1.0" encoding="UTF-8" ?>
<web-app>
  <servlet>
    <description></description>
    <display-name>itemManager</display-name>
    <servlet-name>itemManager</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemManager</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemSelector</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemSelector</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemInsertion</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemInsertion</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemInsertion2</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemInsertion2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemEditor</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemEditor</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemUpdate</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemUpdate</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemUpdate2</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemUpdate2</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>itemDeletion</servlet-name>
    <servlet-class>com.billies_works.javaweb.chapter17.ItemDeletion</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>itemManager</servlet-name>
    <url-pattern>/ItemManager</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemSelector</servlet-name>
    <url-pattern>/ItemSelector</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemInsertion</servlet-name>
    <url-pattern>/ItemInsertion</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemInsertion2</servlet-name>
    <url-pattern>/ItemInsertion2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemEditor</servlet-name>
    <url-pattern>/ItemEditor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemUpdate</servlet-name>
    <url-pattern>/ItemUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemUpdate2</servlet-name>
    <url-pattern>/ItemUpdate2</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>itemDeletion</servlet-name>
    <url-pattern>/ItemDeletion</url-pattern>
  </servlet-mapping>

  <resource-ref>
    <description>DB Connection</description>
    <res-ref-name>jdbc/datasource</res-ref-name>
    <res-type>javax.sql.DataSource</res-type>
    <res-auth>Container</res-auth>
  </resource-ref>
</web-app>

